{% extends 'base.html' %}

{% block conteudo %}
  <p>{{ user.first_name }}'s Profile</p>
  <span><a href="/meu_delicious/novo_favorito">Novo Favorito</a></span>
  <ul>
  {% for favorito in favoritos %}
	<li><a href='{{ favorito.url }}'>{{ favorito }}</a></li>
	{% empty %}
	<p>Voce nao tem Favoritos. <a href='/meu_delicious/novo_favorito'>Clique Aqui</a> para cadastrar o primeiro!</p>
  {% endfor %}
{% endblock conteudo %}
